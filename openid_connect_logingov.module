 require __DIR__ . '/vendor/autoload.php';

/**
 * Implements hook_ctools_plugin_directory().
 */
function openid_connect_logingov_ctools_plugin_directory($owner, $plugin_type) {
  if ($owner == 'openid_connect') {
    return 'plugins/' . $plugin_type;
  }
}
